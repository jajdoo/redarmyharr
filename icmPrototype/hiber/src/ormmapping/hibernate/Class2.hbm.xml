<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="hibernate.Class2" table="Class2" lazy="false">
		<id name="id2" column="Id2" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="parent" cascade="lock" column="Class1Id1" class="hibernate.Class1" not-null="true" lazy="no-proxy" access="field">
