<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="hibernate.Class1" table="Class1" lazy="false">
		<id name="id1" column="Id1" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="text" column="Text" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM_Children" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="Class1Id1" not-null="true"/>
			<one-to-many class="hibernate.Class2"/>
		</set>
	</class>
</hibernate-mapping>
